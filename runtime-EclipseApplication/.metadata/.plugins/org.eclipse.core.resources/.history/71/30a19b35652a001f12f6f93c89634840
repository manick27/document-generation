document $name allInOne=false {
	with $s in $studentsC {
		page ($s.name) { 
			//title: $name
			//subtitle: "Bulletin" class="red"
			//subsubtitle: "1234" 
			row {
				col img class="image" src:$s.profile alt:"photo" class="logo"
				col colspan: 3 $s.name class="pl-5"
			}
			
			row {
				with $h in $header {  
					col $h class="bold italic center"				
				}
			}
			
			$i from 0 to $s.courses { 
				row {
					col $s.courses[$i] class="italic"
					col $s.marks[$i] class="center"
					col SUM($s.coefs[$i]) class="center"
					col PROD($s.marks[$i], $s.coefs[$i]) class="center notexcoef"
				}
			}
			
			row { 
				col $footer[0] class="italic bold center"
				col SUM($s.marks) class="center"
				col SUM($s.coefs) class="center totalcoefs"
				col SUM(.notexcoef) class="center bold totalpoints"
			}
			
			row {
				col SUM($footer[1]) class="italic bold center"
				col DIV(.totalpoints, .totalcoefs) class="center moyenne"
				col $footer[2] class="center" 
				if(.moyenne < 12) then {
					col "Non" class="center bold"
				} else {
					col "Oui" class="center bold"
				}
			}
		}
	}
}
