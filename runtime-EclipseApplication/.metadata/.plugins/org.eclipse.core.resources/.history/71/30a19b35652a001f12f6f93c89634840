  .red {
  	color: red;
  }
  .blue {
  	color: blue;
  }
  .bold {
  	font-weight: bold;
  }
  .center{
  	text-align: center;
  }
  .italic{
    font-style: italic;
  }
  .pl-5{
  	padding-left: 5px;
  }
  .image {
  	width: 50px;
  }
  table {
  	margin: 10px;
  	margin-bottom: 30px;
  }
  .logo {
  	display: flex;
	justify-content: center; 
	align-items: center;     
	height: 100%;            
	}
}